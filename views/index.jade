extends layout

block content


	div.container
		div.row
			div.col-lg-12(style="font-family: 'Montserrat', sans-serif")
				
				h1= title
				p Welcome to #{title} Impact Calculator, a KPCB Fellows Challenge Project


	div.container
		div.row
			div.col-lg-12(style="font-family: 'Montserrat', sans-serif")
				form(onsubmit="return redirect();")
					paper-input-decorator(label="Enter the name of a lender")
						input(id="name", is="core-input")

				h2 ID: #{lender} / Name: #{name}
				
			div.col-lg-12
				h1 PLACE HOLDER FOR POLYMER